<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.hntrip.root.file.mapper.FileMapper">
	<resultMap type="com.hntrip.root.file.dto.FileDTO" id="file">
		<result property="writeNo" column="wirte_no"/>
		<result property="fileName" column="file_name"/>
	</resultMap>
	<insert id="insert">
 		<foreach collection="list" item="list" separator=" " open="insert all" close="select * from dual">
 		 into file_name (file_name, write_no)
 		 values
 		 (#{list.fileName}, #{list.writeNo})
 		 </foreach>
 	</insert>
 	
 	<delete id="delete">
 		delete from file_name where write_no = #{writeNo}
 	</delete>
 	
 	<select id="findBywriteNo" resultMap="file">
 		select from file_name where wirte_no = #{writeNo}
 	</select>
</mapper>